{
  "name": "interview-express",
  "version": "0.1.0",
  "description": "Provides a standardized framework for interview technical review",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "dev:watch": "nodemon",
    "lint": "eslint .",
    "test": "npm run lint && jest ./src",
    "test:coverage": "npm run lint && jest --collect-coverage",
    "clean": "rimraf dist/*",
    "build": "npm run clean && tsc",
    "start": "npm ls --depth=0 && node ./dist/index.js",
    "graphql-codegen": "graphql-codegen --config codegen.yml"
  },
  "contributors": [
    "Lance Gliser <lance.gliser@torch.ai> (https://torch.ai/)"
  ],
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "1.13.5",
    "@graphql-codegen/typescript": "^1.16.3",
    "@graphql-codegen/typescript-resolvers": "^1.16.3",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/express-winston": "^3.0.4",
    "@types/jest": "^26.0.10",
    "@types/mime-types": "^2.1.0",
    "@types/supertest": "^2.0.9",
    "@types/type-is": "^1.6.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.0",
    "apollo-boost": "^0.4.9",
    "dotenv": "^8.6.0",
    "eslint": "^7.8.1",
    "jest": "^26.4.0",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.27",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.2",
    "rimraf": "^2.7.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^6.2.6",
    "@graphql-tools/load": "^6.2.5",
    "@graphql-tools/schema": "^7.0.0",
    "apollo-server": "^2.19.0",
    "apollo-server-express": "^2.25.2",
    "cors": "^2.8.5",
    "express-healthcheck": "^0.1.0",
    "express-winston": "^4.0.5",
    "form-data": "^3.0.0",
    "graphql": "^15.4.0",
    "graphql-upload": "^12.0.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2"
  }
}
